      "Bash(pnpm exec tailwindcss:*)",
      "Bash(node --version:*)",
      "Bash(npm --version:*)",
      "Bash(pnpm --version:*)",
      "Bash(git log:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git show:*)",
      "Bash(git rev-parse:*)",
      "Bash(git check-ignore:*)",
      "Bash(gh pr list:*)",
      "Bash(brew list:*)",
      "Bash(brew info:*)",
      "Bash(claude:*)",
      "Bash(forge fmt:*)",
      "Bash(cast sig:*)",
      "Bash(cast balance:*)",
      "Bash(cast code:*)",
      "Bash(cast call:*)",
      "Read(//tmp/**)",
      "Read(//Applications/**)",
      "WebSearch",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:raw.githubusercontent.com)",
      "WebFetch(domain:www.npmjs.com)",
      "WebFetch(domain:code.claude.com)",
      "WebFetch(domain:viem.sh)",
      "WebFetch(domain:eips.ethereum.org)",
      "WebFetch(domain:docs.metamask.io)",
      "WebFetch(domain:docs.uniswap.org)",
      "WebFetch(domain:docs.langchain.com)",
      "WebFetch(domain:platform.openai.com)",
      "WebFetch(domain:blog.ethereum.org)",
      "WebFetch(domain:reactbits.dev)",
      "mcp__shadcn__get_project_registries",
      "mcp__shadcn__search_items_in_registries",
      "mcp__shadcn__view_items_in_registries",
      "mcp__shadcn__list_items_in_registries",
