      "mcp__playwright__browser_take_screenshot",
      "mcp__playwright__browser_snapshot",
      "mcp__playwright__browser_click",
      "mcp__playwright__browser_close",
      "mcp__context7__resolve-library-id",
      "mcp__context7__get-library-docs",
      "Skill(docx)",
      "Bash(npx shadcn@latest:*)"
    ],
    "deny": [],
    "ask": [],
    "defaultMode": "default"
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "afplay ~/Library/Sounds/task-finished.wav"
          },
          {
            "type": "command",
            "command": "INPUT=$(cat) && TRANSCRIPT=$(echo \"$INPUT\" | jq -r '.transcript_path // empty') && CWD=$(echo \"$INPUT\" | jq -r '.cwd // empty') && REPO=$(basename \"$CWD\") && TMUX_WIN=$(tmux display-message -t \"$TMUX_PANE\" -p '#I' 2>/dev/null) && PREFIX=\"[$REPO${TMUX_WIN:+ | w$TMUX_WIN}]\" && sleep 1 && MSG=$(tail -100 \"$TRANSCRIPT\" 2>/dev/null | jq -r 'select(.type==\"assistant\") | .message.content[] | select(.type==\"text\") | .text' 2>/dev/null | tail -1 | head -c 120) && echo \"$PREFIX ${MSG:-elpabl0, your turn}\" > /tmp/claude-notif-msg.txt"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "afplay ~/Library/Sounds/user-send-message.wav"
          },
          {
            "type": "command",
            "command": "echo 'IMPORTANT: Before proposing any fix, implementation, or code change: (1) Read and understand existing code first, (2) Verify your analysis with evidence from the codebase, (3) Never rely on assumptions about how code works, (4) Be 100% confident before proposing changes.'"
          }
        ]
      }
    ],
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "afplay ~/Library/Sounds/important-notif.wav"
          },
          {
            "type": "command",
            "command": "INPUT=$(cat) && CWD=$(echo \"$INPUT\" | jq -r '.cwd // empty') && REPO=$(basename \"$CWD\") && TMUX_WIN=$(tmux display-message -t \"$TMUX_PANE\" -p '#I' 2>/dev/null) && PREFIX=\"[$REPO${TMUX_WIN:+ | w$TMUX_WIN}]\" && MSG=$(echo \"$INPUT\" | jq -r '.message // empty') && echo \"$PREFIX ${MSG:-Claude needs your attention}\" > /tmp/claude-notif-msg.txt"
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "afplay ~/Library/Sounds/session-start-short.wav"
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "echo 'REMINDER: Your context window will be automatically compacted as it approaches its limit. Never stop tasks early due to token budget concerns. Always complete tasks fully, even if the end of your budget is approaching.'"
          }
        ]
      }
    ],
    "PostToolUse": [],
    "SubagentStop": []
  },
  "statusLine": {
    "type": "command",
    "command": "bash ~/.claude/statusline.sh"
  },
  "enabledPlugins": {
    "document-skills-docx@awesome-claude-skills": true,
    "document-skills-pdf@awesome-claude-skills": true,
    "supabase@claude-plugins-official": false,
    "ralph-wiggum@claude-plugins-official": true,
    "ralph-loop@claude-plugins-official": true,
    "swift-lsp@claude-plugins-official": true,
    "code-simplifier@claude-plugins-official": true,
    "pragma@pragma-marketplace": true
  },
  "spinnerTipsEnabled": false,
  "spinnerVerbs": {
    "mode": "replace",
    "verbs": [
      "Ikuzo", "Shiraberu", "Mitsuketai", "Tatakae",
      "Hwaiting", "Daebak", "Ppalli ppalli", "Gaja", "Chincha", "Omo",
      "Unhinged", "Sigmaing", "Leroy Jenkins", "No chill",
      "Brewing", "Percolating", "Espresso mode", "Double shot", "Caffeinating",
      "Dripping", "Fourth cup", "Over-extracted", "Cold Brew Mode", "Pulling a shot",
      "Pour Over", "Jittering", "Overcaffeinated", "Americano mode", "Turbo Shot", "Ristretto",
      "Clutching", "Speedrunning", "Farming", "Mogging", "Buffing", "Respawning",
      "Boss Fight", "Critting", "GG EZ", "Combo Breaking", "Aggro",
      "WAGMI", "Aping in", "Bridging", "Minting", "Stacking", "LFG ser",
      "Pumping", "Diamond Handing", "Degen mode", "To the moon",
      "Clawdmaxxing"
    ]
  },
  "alwaysThinkingEnabled": true,
  "skipDangerousModePermissionPrompt": true
}
